{
	"main": "boot.js",
	"android": {
		"v8Flags": "--expose_gc",
		"profilerOutputDir": "",
		"gcThrottleTime": 500,
		"memoryCheckInterval": 10,
		"freeMemoryRatio": 0.50,
		"markingMode": "none",
		"maxLogcatObjectSize": 1024,
		"forceLog": true,
		"suppressCallJSMethodExceptions": false,
		"enableLineBreakpoints": false,
		"enableMultithreadedJavascript": true
	},
	"discardUncaughtJsExceptions": false
}